{
  "manifest_version": 3,
  "name": "WebMind Map",
  "version": "1.0",
  "description": "将浏览历史以思维导图的形式展示",
  "permissions": [
    "history",
    "tabs",
    "favicon"
  ],
  "action": {
    "default_title": "WebMind Map"
  },
  "background": {
    "service_worker": "background.js"
  },
  "web_accessible_resources": [{
    "resources": ["lib/*"],
    "matches": ["<all_urls>"]
  }]
} 